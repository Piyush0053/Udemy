{
    "manifest_version": 3,
    "name": "Adult Content Blocker",
    "version": "1.1",
    "description": "Blocks adult websites",
    "permissions": ["declarativeNetRequest", "storage", "tabs"],
    "host_permissions": ["<all_urls>"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    }
  }
  